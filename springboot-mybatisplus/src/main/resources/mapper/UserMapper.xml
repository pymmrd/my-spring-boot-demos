<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jifeng.springbootmybatisplus.mapper.UserMapper">
    <select id="getUserByName" parameterType="String" resultType="com.jifeng.springbootmybatisplus.entity.User">
        SELECT * FROM TBL_USER WHERE name = #{name}
    </select>
</mapper>
